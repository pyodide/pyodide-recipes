https://github.com/sarah-quinones/faer-rs/issues/173

---
 Cargo.toml | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/Cargo.toml b/Cargo.toml
index 5b78c75..b88333a 100644
--- a/Cargo.toml
+++ b/Cargo.toml
@@ -121,11 +121,11 @@ optional = true
 # -------------------------------
 
 [dependencies.faer]
-version = "0.21.9"
+version = "0.22.0"
 optional = true 
 
 [dependencies.faer-traits]
-version = "0.21.5"
+version = "0.22.0"
 optional = true 
 
 [dependencies.pardiso-wrapper]
